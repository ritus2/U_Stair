{
  "name": "u_stair",
  "productName": "u_stair",
  "version": "1.0.0",
  "description": "A grade curriculum schedule manager",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "electron .",
    "pack": "electron-builder --dir",
    "distw": "electron-builder --win",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "ustair",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "pacman"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi",
        "squirrel"
      ],
      "icon": "build/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afmorenosa/U_Stair.git"
  },
  "keywords": [
    "Universitary",
    "Schedule",
    "Grade"
  ],
  "author": "And≈ïes Felipe Moreno Sarria <afmorenosa@unal.edu.co>",
  "email": "afmorenosa@unal.edu.co",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/afmorenosa/U_Stair/issues"
  },
  "homepage": "https://github.com/afmorenosa/U_Stair#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "a"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^9.3.4",
    "electron-builder": "^22.9.1",
    "electron-tabs": "^0.15.0",
    "fs-extra": "^9.0.1",
    "sqlite3": "^5.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "designmodo-flat-ui": "^2.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
